{
  "name": "soundcloud-manager",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@ant-design/icons": "^4.3.0",
    "antd": "^4.9.4",
    "axios": "^0.21.0",
    "dayjs": "^1.9.7",
    "parcel": "2.0.0-nightly.485",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "scripts": {
    "dev": "concurrently \"yarn start\" \"yarn api\"",
    "start": "parcel serve ./public/index.html --host localhost",
    "api": "cd ./api/flask_soundcloud_env/Scripts/ && activate.bat  &&  python -m flask run",
    "build": "parcel build ./public/index.html --dist-dir build"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "babel-plugin-import": "^1.13.3",
    "concurrently": "^5.3.0",
    "npm-check-updates": "^10.2.5"
  }
}
